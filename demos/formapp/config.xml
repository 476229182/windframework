<?xml version="1.0" encoding="UTF-8"?>
<WIND>
	<app isGlobal='true'>
		<name>FormDemo</name>
		<rootPath></rootPath>
		<configPath>FormDemo:compile</configPath>
	</app>
	<modules>
		<module name='default'>
			<path>controllers</path>
		</module>
		<module name='other'>
			<path>otherControllers</path>
		</module>
	</modules>
	<filters>
		<filter name="WindFormFilter">
			<filterPath>WIND:component.form.WindFormFilter</filterPath>
		</filter>
	</filters>
	<templates>
		<template>
			<dir>templates</dir>
			<default>index</default>
			<ext>html</ext>
			<resolver>default</resolver>
			<isCache>false</isCache>
			<cacheDir>cache</cacheDir>
			<compileDir>compile</compileDir>
		</template>
	</templates>
</WIND>