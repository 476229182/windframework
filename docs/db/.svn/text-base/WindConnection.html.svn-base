<li><div class="src-line">&nbsp;<span class="src-var">$stm&nbsp;</span>=&nbsp;<span class="src-var">$connection</span><span class="src-sym">-&gt;</span><a href="../db/WindConnection.html#methodcreateStatement">createStatement</a><span class="src-sym">(</span><span class="src-str">'SELECT&nbsp;*&nbsp;FROM&nbsp;{members}&nbsp;WHERE&nbsp;uid&lt;=:uid'</span><span class="src-sym">,&nbsp;</span><span class="src-id">true</span><span class="src-sym">)</span><span class="src-sym">;</span></div></li>
<li><div class="src-line">&nbsp;<span class="src-var">$stm</span><span class="src-sym">-&gt;</span><span class="src-id">queryAll</span><span class="src-sym">(</span><span class="src-sym">)</span><span class="src-sym">;</span></div></li>
