<?xml version="1.0" encoding="UTF-8"?>
<WIND>
	<app isGlobal='true'>
		<name></name>
		<rootPath></rootPath>
		<configPath></configPath>
	</app>
	<applications>
		<application name='web'>
			<class>WIND:core.WindWebApplication</class>
		</application>
		<application name='command'>
			<class>WIND:core.WindCommandApplication</class>
		</application>
	</applications>
	<modules>
		<module name='default'>
			<path>actionControllers</path>
		</module>
		<module name='other'>
			<path>otherControllers</path>
		</module>
	</modules>
	<errorMessage>
		<errorAction>WIND:core.WindErrorAction</errorAction>
	</errorMessage>
	<filters isMerge='true'>
		<filter name="WindFormFilter">
			<filterPath>WIND:component.form.WindFormFilter</filterPath>
		</filter>
	</filters>
	<template>
		<dir>template</dir>
		<default>index</default>
		<ext>htm</ext>
		<resolver>default</resolver>
		<isCache>false</isCache>
		<cacheDir>cache</cacheDir>
		<compileDir>compile</compileDir>
	</template>
	<viewerResolvers isMerage='true'>
		<resolver name='default'>WIND:component.viewer.WindViewer</resolver>
		<resolver name='pw'>WIND:component.viewer.WindPWViewer</resolver>
		<resolver name='smarty'>libs.WindSmarty</resolver>
	</viewerResolvers>
	<router>
		<parser>url</parser>
	</router>
	<routerParsers isMerge='true'>
		<parser name='url'>
			<rule>
				<action name='a'>run</action>
				<controller name='c'>index</controller>
				<module name='m'>default</module>
			</rule>
			<path>WIND:component.router.WindUrlBasedRouter</path>
		</parser>
	</routerParsers>
</WIND>