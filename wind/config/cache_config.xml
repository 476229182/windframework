<?xml version="1.0" encoding="UTF-8"?>
<cache>
		<!--memcache配置-->
		<servers>
			<server name='phpwind'>
				<host>127.0.0.1</host>
				<port>11211</port>
			</server>
			<server name='cac'>
				<host>127.0.0.1</host>
				<port>11211</port>
			</server>
		</servers>
		<compress>2</compress>
		<!--数据库缓存配置-->
		<cachetable>
			<name>pw_cache</name>
			<field>
				<key>name</key>
				<value>cache</value>
				<expire>time</expire>
			</field>
			<expirestrage>true</expirestrage>
		</cachetable>
		<!--文件缓存配置-->
		<cache-dir>E:\www\bbs\wind\demos\helloworld\compile\cache</cache-dir>
		<cache-level>0</cache-level>
		<cache-suffix>.txt</cache-suffix>
		<!--通用配置-->
		<security>true</security>
		<expires>10</expires>
		<!--
		<dependency>Wind:component.cache.dependency.WindSqlCacheDependency</dependency>
		-->
</cache>
